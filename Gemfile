source "https://rubygems.org"

# Use stable Jekyll version that worked before
gem "jekyll", "~> 4.3.0"
gem "nokogiri", "~> 1.16.0"

group :jekyll_plugins do
  gem 'jekyll-sitemap'
  gem 'jekyll-seo-tag'
  gem 'kramdown-parser-gfm'
end

# Development and testing
group :development, :test do
  gem 'html-proofer'
end
